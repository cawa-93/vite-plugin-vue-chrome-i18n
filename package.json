{
  "name": "vite-plugin-vue-chrome-i18n",
  "version": "0.0.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./getMessage.js": {
      "import": {
        "types": "./dist/getMessage.d.ts",
        "default": "./dist/getMessage.js"
      }
    }
  },
  "keywords": [
    "vite-plugin"
  ],
  "scripts": {
    "build": "tsup ./src/index.ts ./src/getMessage.ts --format esm --dts",
    "playground:dev": "cd ./playground && vite",
    "playground:build": "cd ./playground && vue-tsc && vite build --minify=false",
    "test": "vitest"
  },
  "devDependencies": {
    "@types/node": "^20.4.4",
    "@vitejs/plugin-vue": "^4.2.3",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.0",
    "vitest": "^0.33.0",
    "vue": "^3.3.4",
    "vue-tsc": "^1.8.3"
  },
  "peerDependencies": {
    "@types/chrome": "^0.0.241"
  }
}
